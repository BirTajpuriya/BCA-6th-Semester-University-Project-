<%- include('../../partials/adminHeader') -%>
<div class="content pb-0">
    <div class="animated fadeIn">
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-header"><strong>Products</strong><small> Form</small></div>
                    <div class="card-body card-block">
                        <form action="/addcategory" method="post">
                           
                            <div class="form-group">
                                <label for="company" class=" form-control-label">Category Name</label>
                                <input type="text" name="name" id="company" placeholder="Enter Category name" class="form-control" value="" required>
                            </div>
                           
                            


                            <button id="payment-button" type="submit" name="submit" class="btn btn-lg btn-info btn-block">
                                <span id="payment-button-amount">Submit</span>
                            </button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


